{
  "projectId": "the-recipes",
  "endpoint": "https://nyc.cloud.appwrite.io/v1",
  "projectName": "The Recipes",
  "settings": {
    "services": {
      "account": true,
      "avatars": true,
      "databases": true,
      "locale": true,
      "health": true,
      "storage": true,
      "teams": true,
      "users": true,
      "sites": true,
      "functions": true,
      "graphql": true,
      "messaging": true
    },
    "auth": {
      "methods": {
        "jwt": true,
        "phone": true,
        "invites": true,
        "anonymous": true,
        "email-otp": true,
        "magic-url": true,
        "email-password": true
      },
      "security": {
        "duration": 31536000,
        "limit": 0,
        "sessionsLimit": 10,
        "passwordHistory": 0,
        "passwordDictionary": false,
        "personalDataCheck": false,
        "sessionAlerts": false,
        "mockNumbers": []
      }
    }
  },
  "functions": [
    {
      "$id": "the-recipes-ai",
      "execute": ["users"],
      "name": "The Recipes AI",
      "enabled": true,
      "logging": true,
      "runtime": "go-1.23",
      "scopes": [],
      "events": [],
      "schedule": "",
      "timeout": 15,
      "entrypoint": "main.go",
      "commands": "",
      "specification": "s-0.5vcpu-512mb",
      "path": "functions/the-recipes-ai"
    },
    {
      "$id": "the-recipes-accounts",
      "execute": ["users"],
      "name": "The Recipes Accounts",
      "enabled": true,
      "logging": true,
      "runtime": "bun-1.1",
      "scopes": ["users.write"],
      "events": [],
      "schedule": "",
      "timeout": 15,
      "entrypoint": "src/index.ts",
      "commands": "bun i",
      "specification": "s-0.5vcpu-512mb",
      "path": "functions/the-recipes-accounts"
    }
  ],
  "sites": [
    {
      "$id": "the-recipes-site",
      "name": "The Recipes Site",
      "enabled": true,
      "logging": true,
      "framework": "astro",
      "timeout": 30,
      "installCommand": "npm install",
      "buildCommand": "npm run build",
      "outputDirectory": "./dist",
      "specification": "s-0.5vcpu-512mb",
      "buildRuntime": "node-22",
      "adapter": "ssr",
      "fallbackFile": "",
      "path": "website"
    }
  ],
  "tablesDB": [
    {
      "$id": "principal",
      "name": "principal",
      "enabled": true
    }
  ],
  "tables": [
    {
      "$id": "recipes",
      "$permissions": ["create(\"users\")"],
      "databaseId": "principal",
      "name": "recipes",
      "enabled": true,
      "columns": [
        {
          "key": "title",
          "type": "string",
          "required": true,
          "array": false,
          "size": 100,
          "default": null,
          "encrypt": false
        },
        {
          "key": "description",
          "type": "string",
          "required": true,
          "array": false,
          "size": 250,
          "default": null,
          "encrypt": false
        },
        {
          "key": "image",
          "type": "string",
          "required": false,
          "array": false,
          "size": 150,
          "default": null,
          "encrypt": false
        },
        {
          "key": "ingredients",
          "type": "string",
          "required": false,
          "array": true,
          "size": 500,
          "default": null,
          "encrypt": false
        },
        {
          "key": "directions",
          "type": "string",
          "required": false,
          "array": true,
          "size": 500,
          "default": null,
          "encrypt": false
        },
        {
          "key": "preparationTime",
          "type": "integer",
          "required": true,
          "array": false,
          "min": 0,
          "max": 9223372036854775807,
          "default": null
        },
        {
          "key": "ownerId",
          "type": "string",
          "required": true,
          "array": false,
          "size": 50,
          "default": null,
          "encrypt": false
        },
        {
          "key": "isPublic",
          "type": "boolean",
          "required": false,
          "array": false,
          "default": false
        }
      ],
      "indexes": []
    }
  ],
  "buckets": [
    {
      "$id": "recipes",
      "$permissions": ["create(\"users\")"],
      "fileSecurity": true,
      "name": "recipes",
      "enabled": true,
      "maximumFileSize": 50000000,
      "allowedFileExtensions": ["jpg", "png", "webp", "jpeg"],
      "compression": "none",
      "encryption": true,
      "antivirus": true
    }
  ]
}
